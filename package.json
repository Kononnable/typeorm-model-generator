{
  "name": "@zdhsoft/tmg",
  "version": "0.0.5",
  "description": "这个是从https://github.com/Kononnable/typeorm-model-generator.git fork过来的项目，目的在于增加定制化的一些功能和要求",
  "bin": "bin/tmg",
  "main": "./dist/src/library.js",
  "types": "./dist/src/library.d.ts",
  "scripts": {
    "start": "ts-node ./src/index.ts",
    "help": "ts-node ./src/index.ts --help",
    "build": "npm run clean && tsc && ncp src/templates/ dist/src/templates/ && ncp package.json dist/package.json",
    "prepare": "npm run build",
    "pretest": "tsc --noEmit",
    "test": "nyc --reporter=lcov ts-node ./node_modules/mocha/bin/_mocha test/**/*.test.ts --config test/configs/mocha.json -- --bail",
    "posttest": "eslint ./src/**/*.ts  ./test/**/*.ts",
    "clean": "rimraf coverage output dist",
    "prettier": "prettier --write ./src/*.ts ./src/**/*.ts"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/zdhsoft/typeorm-model-generator.git"
  },
  "author": "zdhsoft, 原作者:Kononnable",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/zdhsoft/typeorm-model-generator/issues"
  },
  "homepage": "https://github.com/zdhsoft/typeorm-model-generator.git",
  "dependencies": {
    "change-case": "^4.1.2",
    "fs-extra": "^10.1.0",
    "handlebars": "^4.7.7",
    "inquirer": "^9.1.0",
    "mssql": "^7.3.5",
    "mysql2": "^2.3.3",
    "pg": "^8.7.3",
    "pluralize": "^8.0.0",
    "prettier": "^2.7.1",
    "reflect-metadata": "^0.1.13",
    "sqlite3": "^5.0.11",
    "typeorm": "^0.3.7",
    "xmcommon": "^1.0.8",
    "yargs": "^17.5.1"
  },
  "devDependencies": {
    "@types/array.prototype.flatmap": "^1.2.2",
    "@types/chai": "^4.3.3",
    "@types/chai-as-promised": "^7.1.5",
    "@types/chai-subset": "^1.3.3",
    "@types/eslint": "^8.4.5",
    "@types/fs-extra": "^9.0.13",
    "@types/handlebars": "^4.1.0",
    "@types/inquirer": "^9.0.1",
    "@types/mocha": "^9.1.1",
    "@types/mssql": "^8.0.3",
    "@types/mysql": "^2.15.21",
    "@types/node": "^18.7.6",
    "@types/oracledb": "^5.2.3",
    "@types/pg": "^8.6.5",
    "@types/pluralize": "0.0.29",
    "@types/prettier": "^2.7.0",
    "@types/sinon": "^10.0.13",
    "@types/sqlite3": "^3.1.8",
    "@types/yargs": "^17.0.11",
    "@typescript-eslint/eslint-plugin": "^5.33.1",
    "@typescript-eslint/parser": "^5.33.1",
    "@typescript-eslint/typescript-estree": "^5.33.1",
    "array.prototype.flatmap": "^1.3.0",
    "chai": "^4.3.6",
    "chai-as-promised": "^7.1.1",
    "chai-subset": "^1.6.0",
    "codecov": "^3.8.3",
    "dotenv": "^16.0.1",
    "eslint": "^8.22.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-import": "^2.26.0",
    "husky": "^8.0.1",
    "lint-staged": "^13.0.3",
    "mocha": "^10.0.0",
    "ncp": "^2.0.0",
    "nyc": "^15.1.0",
    "rimraf": "^3.0.2",
    "sinon": "^14.0.0",
    "sinon-chai": "^3.7.0",
    "ts-node": "^10.9.1",
    "typescript": "^4.7.4",
    "yn": "^4.0.0"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run prettier && git update-index --again"
    }
  },
  "nyc": {
    "check-coverage": false,
    "all": true,
    "extension": [
      ".ts",
      ".tsx"
    ],
    "lines": "50",
    "include": [
      "src/**/!(*.test.*).[tj]s?(x)"
    ]
  }
}
